{
  "name": "ao-agent-dev",
  "displayName": "AO Agent Dev",
  "description": "Visualize, edit and improve agent workflows.",
  "repository": "https://github.com/ao-agent-ops/ao-dev",
  "version": "0.0.6",
  "publisher": "agentops",
  "license": "Apache-2.0",
  "icon": "icon.png",
  "readme": "PKG_README.md",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "graphExtension.showGraph",
        "title": "Show Graph Panel",
        "category": "Graph Extension"
      },
      {
        "command": "graphExtension.signIn",
        "title": "Sign In with Google",
        "category": "Graph Extension"
      },
      {
        "command": "graphExtension.signOut",
        "title": "Sign Out",
        "category": "Graph Extension"
      }
    ],
    "configuration": {
      "title": "AO",
      "properties": {
        "ao.authServerUrl": {
          "type": "string",
          "default": "http://localhost:5958",
          "description": "URL of the authentication server"
        },
        "ao.pythonServerHost": {
          "type": "string",
          "default": "127.0.0.1",
          "description": "Host of the Python develop server"
        },
        "ao.pythonServerPort": {
          "type": "number",
          "default": 5959,
          "description": "Port of the Python develop server"
        }
      }
    },
    "views": {
      "graph-extension": [
        {
          "type": "webview",
          "id": "graphExtension.graphView",
          "name": "Graph View",
          "icon": "$(graph)",
          "contextualTitle": "Graph View",
          "retainContextWhenHidden": true
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "graph-extension",
          "title": "Graph Extension",
          "icon": "$(type-hierarchy)"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package && rm -f dist/*.map",
    "compile": "webpack",
    "build": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^16.18.126",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/vscode": "^1.74.0",
    "@types/ws": "^8.5.10",
    "css-loader": "^6.11.0",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.2",
    "typescript": "^5.9.2",
    "webpack": "^5.99.9",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@react-pdf-viewer/core": "^3.12.0",
    "@react-pdf-viewer/default-layout": "^3.12.0",
    "@supabase/supabase-js": "^2.39.0",
    "@tisoap/react-flow-smart-edge": "^2.0.0",
    "@vscode/codicons": "^0.0.42",
    "docx-preview": "^0.3.6",
    "elkjs": "^0.8.2",
    "js-yaml": "^4.1.0",
    "lossless-json": "^4.0.2",
    "mammoth": "^1.9.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-pdf": "^10.0.1",
    "reactflow": "^11.10.0",
    "ws": "^8.14.2"
  }
}
