{
  "name": "vscode-graph-extension",
  "displayName": "Graph Extension",
  "description": "A VS Code extension for visualizing graphs",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:graphExtension.graphView",
    "onCommand:graphExtension.showGraph"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "graphExtension.showGraph",
        "title": "Show Graph Panel",
        "category": "Graph Extension"
      }
    ],
    "views": {
      "graph-extension": [
        {
          "type": "webview",
          "id": "graphExtension.graphView",
          "name": "Graph View",
          "icon": "$(graph)",
          "contextualTitle": "Graph View"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "graph-extension",
          "title": "Graph Extension",
          "icon": "$(graph)"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map"
  },
  "devDependencies": {
    "@types/node": "^16.18.126",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/vscode": "^1.74.0",
    "css-loader": "^6.11.0",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.2",
    "typescript": "^4.9.5",
    "webpack": "^5.99.9",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@tisoap/react-flow-smart-edge": "^2.0.0",
    "elkjs": "^0.8.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reactflow": "^11.10.0"
  }
}
