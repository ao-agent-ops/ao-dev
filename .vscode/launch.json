{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "aco-config",
      "type": "debugpy",
      "request": "launch",
      "module": "cli.aco_config",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "aco-server",
      "type": "debugpy",
      "request": "launch",
      "module": "cli.aco_server",
      "args": ["restart"],
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "aco-launch",
      "type": "debugpy",
      "request": "launch",
      "module": "cli.aco_launch",
      // "args": ["./example_workflows/miroflow_deep_research/single_task.py"],
      // "args": ["./example_workflows/debug_examples/patch_debug.py"],
      "args": ["./example_workflows/debug_examples/openai_add_numbers.py"],
      // "args": ["./example_workflows/debug_examples/openai_chat.py"],
      // "args": ["./example_workflows/debug_examples/openai_async_agents.py"],
      // "args": ["./src/runner/monkey_patching/patches/uuid_patches.py"],
      // "args": ["./src/runner/monkey_patching/patches/uuid_patches.py"],
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "Run Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/src/user_interface"
      ],
      "outFiles": [
        "${workspaceFolder}/src/user_interface/dist/**/*.js"
      ],
      "preLaunchTask": "npm: watch",
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}