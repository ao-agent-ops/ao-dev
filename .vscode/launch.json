{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "develop try.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/agent_copilot/develop_shim.py",
            "console": "integratedTerminal",
            "args": [
                "${workspaceFolder}/testbed/code_repos/try_out/try.py"
            ]
        },
    {
      "name": "Run Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/src/user_interface"
      ],
      "outFiles": [
        "${workspaceFolder}/src/user_interface/dist/**/*.js"
      ],
      "preLaunchTask": "npm: watch",
      "internalConsoleOptions": "openOnSessionStart"
    }
    ]
}