{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": false,
      "purpose": ["debug-test", "debug-in-terminal"]
    },
    // {
    //   "name": "Python: Testing",
    //   "type": "debugpy",
    //   "request": "launch",
    //   "program": "${file}",
    //   "console": "integratedTerminal",
    //   "justMyCode": false,
    // },
    {
      "name": "ao-config",
      "type": "debugpy",
      "request": "launch",
      "module": "ao.cli.ao_config",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "ao-server",
      "type": "debugpy",
      "request": "launch",
      "module": "ao.cli.ao_server",
      "args": ["restart"],
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "ao-tool rerun",
      "type": "debugpy",
      "request": "launch",
      "module": "ao.cli.ao_tool",
      "args": ["rerun"],
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "ao-tool edit-and-rerun",
      "type": "debugpy",
      "request": "launch",
      "module": "ao.cli.ao_tool",
      "args": [
        "edit-and-rerun",
        "--output",
        "{\"content.content\": [{\"text\": \"Is pineapple an acceptable pizza topping?\"}], \"content.role\": \"assistant\"}",
        "--wait",
        "d485e4f2-49db-43cb-aa23-91c515e0499b",
        "c504dbef-16a8-4869-8478-1a31b8f73c4d",
      ],
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "ao-record",
      "type": "debugpy",
      "request": "launch",
      "module": "ao.cli.ao_record",
      // Anthropic examples
      // "args": ["./example_workflows/debug_examples/anthropic/async_debate.py"],
      "args": ["./example_workflows/debug_examples/anthropic/debate.py"],
      // "args": ["./example_workflows/debug_examples/anthropic/document_query.py"],
      // "args": ["./example_workflows/debug_examples/anthropic/image_tool_call.py"],
      // "args": ["./example_workflows/debug_examples/anthropic/inline_docs.py"],
      // Langchain examples
      // "args": ["./example_workflows/debug_examples/langchain/async_agent.py"],
      // "args": ["./example_workflows/debug_examples/langchain/agent.py"],
      // "args": ["./example_workflows/debug_examples/langchain/simple_chat.py"],
      // Together examples
      // "args": ["./example_workflows/debug_examples/together/debate.py"],
      // CrewAI examples
      // "args": ["./example_workflows/debug_examples/crewai/research_writer.py"],
      // "args": ["./example_workflows/debug_examples/crewai/jina_scraper.py"],
      // "args": ["./example_workflows/debug_examples/crewai/multi_tool.py"],
      // "args": ["./example_workflows/debug_examples/crewai/advanced_tools.py"],
      // MCP examples
      // "args": ["./example_workflows/debug_examples/mcp/simple_test.py"],
      // Google/VertexAI examples
      // "args": ["./example_workflows/debug_examples/google/streaming_async.py"],
      // "args": ["./example_workflows/debug_examples/google/streaming.py"],
      // "args": ["./example_workflows/debug_examples/google/gen_image.py"],
      // "args": ["./example_workflows/debug_examples/google/debate_async.py"],
      // "args": ["./example_workflows/debug_examples/google/debate.py"],
      // Ollama examples
      // "args": ["./example_workflows/debug_examples/ollama/native_debate.py"],
      // "args": ["./example_workflows/debug_examples/ollama/debate.py"],
      // OpenAI examples
      // "args": ["./example_workflows/debug_examples/openai/document_query.py"],
      // "args": ["./example_workflows/debug_examples/openai/chat_async.py"],
      // "args": ["./example_workflows/debug_examples/openai/debate.py"],
      // "args": ["./example_workflows/debug_examples/openai/async_debate.py"],
      // "args": ["./example_workflows/debug_examples/openai/tool_call.py"],
      // "args": ["./example_workflows/debug_examples/openai/document_reader.py"],
      // "args": ["./example_workflows/debug_examples/openai/document_writer.py"],
      // "args": ["./example_workflows/debug_examples/openai/chat.py"],
      // "args": ["./example_workflows/debug_examples/openai/async_agents.py"],
      // "args": ["./example_workflows/debug_examples/openai/inline_docs.py"],
      // Subruns examples
      // "args": ["./example_workflows/debug_examples/subruns/multiple_runs_asyncio.py"],
      // "args": ["./example_workflows/debug_examples/subruns/multiple_runs_sequential.py"],
      // "args": ["./example_workflows/debug_examples/subruns/multiple_runs_threading.py"],
      // Miroflow
      // "args": ["./example_workflows/miroflow_deep_research/single_task.py"],
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": {"MIROFLOW_USE_SANDBOX_MODE": "true"}
    },
    {
      "name": "Run Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/src/user_interfaces/vscode_extension"
      ],
      "outFiles": [
        "${workspaceFolder}/src/user_interfaces/vscode_extension/dist/**/*.js"
      ],
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}